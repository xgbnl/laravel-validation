{
  "name": "xgbnl/validation",
  "description": "Scene-based Validator in Laravel.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Elephant\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\": "tests/Unit/App/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "illuminate/validation": "^10.15",
    "illuminate/support": "^10.15",
    "illuminate/http": "^10.15",
    "laravel/framework": "^10.15",
    "phpunit/phpunit": "^10.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Elephant\\Validation\\ValidationServiceProvider"
      ]
    }
  }
}
